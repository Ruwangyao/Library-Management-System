ry-vue\\src\\views\\MessageList\\index.vue"],"sourcesContent":["<template>\n  <div class=\"messageList\">\n    <template v-if=\"messageList && messageList.length > 0\">\n      <el-alert\n          v-for=\"message in messageList\"\n          :key=\"message.id\"\n          :title=\"'时间：'+message.sendDate\"\n          :description=\"message.message\"\n          :closable=\"false\"\n          type=\"info\" effect=\"dark\"\n          style=\"margin-bottom: 10px\"/>\n    </template>\n    <template v-else>\n      \n      <el-alert title=\"无任何消息\" type=\"info\" effect=\"dark\" />\n    </template>\n  </div>\n</template>\n\n<script>\nimport {onBeforeMount, shallowReactive} from \"vue\";\nimport {getUserMsgListApi} from \"@/api/user\";\n\nexport default {\n  name: \"MessageList\",\n  setup(){\n    const messageList = shallowReactive([])\n\n    function getMessage(){\n      getUserMsgListApi()\n        .then(res=>{\n          messageList.push(...res.data.msg)\n        })\n    }\n    onBeforeMount(()=>{\n      getMessage()\n    })\n    return {\n      messageList\n    }\n  }\n}\n</script>\n\n<style scoped lang=\"scss\">\n.messageList{\n  padding: 20px 30px 30px 20px;\n  .el-alert::v-deep{\n    .el-alert__description{\n      font-size: 13px !important;\n    }\n  }\n}\n</style>\n"]}]}