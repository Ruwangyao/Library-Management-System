n    tagList: [],\n  }),\n  getters:{\n    // 获取需要缓存的标签列表\n    getCachedTagList(state){\n      if(state.isShowTagList){\n        return state.tagList.filter(tag => tag.cached === true).map(tag => tag.name)\n      }else{\n        return []\n      }\n    }\n  },\n  actions: {\n    // 切换侧边栏折叠的状态\n    collapseChange(){\n      this.sidebarCollapse = !this.sidebarCollapse\n    },\n\n    /**\n     * 经过检查可以被设置为标签的route对象\n     * @param routeItem\n     */\n    addTag(routeItem){\n      // 如果标签栏的数量大于最大标签，则从头部开始删除部分标签便于添加新标签\n      if(this.tagList.length >= this.maxTagNum){\n        this.delTags(0,this.tagList.length - this.maxTagNum + 1)\n      }\n      // 添加新标签\n      this.tagList.push({\n        name: routeItem.name,\n        title: routeItem.meta.title,\n        path: routeItem.path,\n        // 标识该标签对应的路由是否需要被缓存\n        cached: routeItem.name && routeItem.meta.cached === true\n      })\n    },\n    // 判断是否已经存在某一个路由标签\n    isExistTag(routeItem){\n      return this.tagList.some(item=>{\n        return item.path === routeItem.path\n      })\n    },\n    // 删除标签\n    delTags(start,nums){\n      this.tagList.splice(start,nums)\n    },\n  }\n})\n"],"mappings":";;;;;AAAA,SAAQA,WAAR,QAA0B,OAA1B;AAGA,OAAO,IAAMC,kBAAkB,GAAGD,WAAW,CAAC,iBAAD,EAAmB;EAC9DE,KAAK,EAAE;IAAA,OAAM;MACX;MACAC,eAAe,EAAE,KAFN;MAIX;MACAC,aAAa,EAAE,IALJ;MAMX;MACAC,SAAS,EAAE,CAPA;MAQX;MACAC,OAAO,EAAE;IATE,CAAN;EAAA,CADuD;EAY9DC,OAAO,EAAC;IACN;IACAC,gBAFM,4BAEWN,KAFX,EAEiB;MACrB,IAAGA,KAAK,CAACE,aAAT,EAAuB;QACrB,OAAOF,KAAK,CAACI,OAAN,CAAcG,MAAd,CAAqB,UAAAC,GAAG;UAAA,OAAIA,GAAG,CAACC,MAAJ,KAAe,IAAnB;QAAA,CAAxB,EAAiDC,GAAjD,CAAqD,UAAAF,GAAG;UAAA,OAAIA,GAAG,CAACG,IAAR;QAAA,CAAxD,CAAP;MACD,CAFD,MAEK;QACH,OAAO,EAAP;MACD;IACF;EARK,CAZsD;EAsB9DC,OAAO,EAAE;IACP;IACAC,cAFO,4BAES;MACd,KAAKZ,eAAL,GAAuB,CAAC,KAAKA,eAA7B;IACD,CAJM;;IAMP;AACJ;AACA;AACA;IACIa,MAVO,kBAUAC,SAVA,EAUU;MACf;MACA,IAAG,KAAKX,OAAL,CAAaY,MAAb,IAAuB,KAAKb,SAA/B,EAAyC;QACvC,KAAKc,OAAL,CAAa,CAAb,EAAe,KAAKb,OAAL,CAAaY,MAAb,GAAsB,KAAKb,SAA3B,GAAuC,CAAtD;MAC